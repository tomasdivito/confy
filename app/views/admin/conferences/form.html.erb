<%= simple_form_for [:admin, @conference] do |f| %>

    <%= render :partial => 'shared/errors', locals: { f: f } %>

    <%= f.input :title %>
    <%= f.input :slug %>
    <%= f.input :description %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :place %>
    <%= f.input :url %>
    <%= f.input :image %>

    <%= f.button :submit %>

<% end %>