<%= form_for [:admin, @conference] do |f| %>

<%= render :partial => 'shared/errors', locals: { f: f } %>

<%= f.text_field :title %>
<%= f.text_area :description %>
<%= f.date_field :start_date %>
<%= f.date_field :end_date %>
<%= f.text_field :place %>
<%= f.text_field :url %>
<%= f.file_field :image %>

<%= f.submit %>

<% end %>